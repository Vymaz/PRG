<?php

/**
 *  normalni trida ktera dedi predchozi abstraktni tridu
 */
class ustrednaNguyen extends ustrednyVymazal{
    /**
     *  konstanta ktera se nastavila a uz se nemeni, pristup k ni je staticky
     */
    const TYPE = 1;

   
    /**
     * funkce pro ziskani hodnoty atributu
     * @return type
     */
    public function getNapetiVymazal() {
      return $this->napetiVymazal;
  }
  
/**
 * fuknce ktera nastavi hodnotu atributu
 * @param type $napetiVymazal
 */
   public function setNapetiVymazal($napetiVymazal) {
      $this->napetiVymazal = $napetiVymazal;
  }
}
